# Export cuda related libraries
export PATH=$PATH:/usr/local/cuda/bin
export PATH=$PATH:/usr/local/tensorrt/bin
export PATH=$PATH:/usr/local/cuda/extras/CUPTI/lib64
export PATH=$PATH:/usr/local/cuda/include

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda/extras/CUPTI/lib64
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda/lib64
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda/include
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/tensorrt/lib
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/include
